# Your app's name
EXPO_PUBLIC_APP_NAME="Pet Services"

# Backend API URL (if using Mobigen backend)
EXPO_PUBLIC_API_URL=https://api.mobigen.io

# ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
# OPTIONAL FEATURES
