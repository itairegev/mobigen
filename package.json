{
  "name": "mobigen",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered React Native app generator",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "cd tests && pnpm test",
    "test:api": "cd tests && pnpm test:api",
    "test:services": "cd tests && pnpm test:services",
    "test:e2e": "cd tests && pnpm test:e2e",
    "test:ui": "cd tests && pnpm test:ui",
    "test:coverage": "cd tests && pnpm test:coverage",
    "test:watch": "cd tests && pnpm test:watch",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "generate:ast": "npx ts-node scripts/generate-template-ast.ts",
    "prebuild": "npm run generate:ast",
    "certify": "npx tsx scripts/certify-all-templates.ts",
    "certify:gold": "npx tsx scripts/certify-all-templates.ts --level=gold",
    "certify:template": "npx tsx scripts/certify-all-templates.ts --template",
    "templates:status": "npx tsx scripts/check-template-status.ts",
    "templates:status:detailed": "npx tsx scripts/check-template-status.ts --detailed",
    "templates:analyze": "npx tsx scripts/analyze-templates.ts"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
