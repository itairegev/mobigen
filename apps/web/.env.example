# Database URL (if using Prisma directly)
DATABASE_URL=postgresql://user:password@localhost:5432/mobigen

# NextAuth (if using authentication)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-here
