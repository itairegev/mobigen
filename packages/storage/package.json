{
  "name": "@mobigen/storage",
  "version": "0.1.0",
  "description": "S3 and Git storage layer for Mobigen projects",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/lib-storage": "^3.500.0",
    "simple-git": "^3.22.0",
    "tar": "^7.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/tar": "^6.1.10",
    "typescript": "^5.3.3"
  }
}
