{
  "name": "@mobigen/analytics-db",
  "version": "0.1.0",
  "description": "Database clients and queries for Mobigen analytics",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.10",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./clickhouse": {
      "types": "./dist/clickhouse.d.ts",
      "default": "./dist/clickhouse.js"
    },
    "./timescale": {
      "types": "./dist/timescale.d.ts",
      "default": "./dist/timescale.js"
    },
    "./queries": {
      "types": "./dist/queries.d.ts",
      "default": "./dist/queries.js"
    }
  }
}
