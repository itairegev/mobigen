{
  "name": "@mobigen/ota-updates",
  "version": "0.1.0",
  "description": "OTA (Over-The-Air) updates service for Mobigen using Expo Updates",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mobigen/db": "workspace:*",
    "@mobigen/storage": "workspace:*",
    "@expo/eas-build-job": "^1.0.0",
    "execa": "^9.6.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
