{
  "name": "@mobigen/template-apis",
  "version": "0.1.0",
  "private": true,
  "description": "Unified API client and wrappers for Mobigen templates",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./dogs": {
      "types": "./src/apis/dogs.ts",
      "import": "./src/apis/dogs.ts",
      "default": "./src/apis/dogs.ts"
    },
    "./cats": {
      "types": "./src/apis/cats.ts",
      "import": "./src/apis/cats.ts",
      "default": "./src/apis/cats.ts"
    },
    "./meals": {
      "types": "./src/apis/meals.ts",
      "import": "./src/apis/meals.ts",
      "default": "./src/apis/meals.ts"
    },
    "./news": {
      "types": "./src/apis/news.ts",
      "import": "./src/apis/news.ts",
      "default": "./src/apis/news.ts"
    },
    "./products": {
      "types": "./src/apis/products.ts",
      "import": "./src/apis/products.ts",
      "default": "./src/apis/products.ts"
    },
    "./exercises": {
      "types": "./src/apis/exercises.ts",
      "import": "./src/apis/exercises.ts",
      "default": "./src/apis/exercises.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1"
  },
  "devDependencies": {
    "typescript": "~5.6.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
